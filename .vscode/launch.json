{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Colmap2O3D",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/colmap_utils/colmap2open3d.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Camera Pose",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/onepose_utils/data_prepare.py",
            "console": "integratedTerminal",
            "args": [
                "--dataset",
                "${workspaceFolder}/data/kcup/kcup_1",
                "--nerf_path=${workspaceFolder}/external/instant-ngp",
                "--aabb_scale=4",
                "--fps=10",
                "--redo_sfm"
            ],
            "justMyCode": true
        },
        {
            "name": "2D Mask",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/detic_utils/consistent_mask.py",
            "python": "/home/robot-learning/Software/miniconda3/envs/pytracking/bin/python",
            "args": [
                "--dataset",
                "kcup/kcup_1"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "/home/robot-learning/Projects/pytracking/pytracking"
        },
        {
            "name": "Seg Mask",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/detic_utils/detic_segmentation.py",
            "args": [
                "--detic_path",
                "${workspaceFolder}/external/Detic", 
                "--dataset", 
                "kcup/kcup_1"],
            "cwd": "${workspaceFolder}/external/Detic",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "SFM",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/OnePose/run_ext.py",
            "args": [
                "+preprocess=sfm_spp_spg_custom.yaml",
                "redo=True"
            ],
            "cwd": "${workspaceFolder}/external/OnePose",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose inference",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/OnePose/inference_online.py",
            "args": [
                "+experiment=test_GATsSPG_custom.yaml",
                "object_detect_mode=GT_box",
                "save_wis3d=True"
            ],
            "cwd": "${workspaceFolder}/external/OnePose",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}