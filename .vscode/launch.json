    "version": "0.2.0",
    "configurations": [
        {
            "name": "Check Poses",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/onepose_utils/check_poses.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose prepare",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/onepose_utils/data_prepare.py",
            "console": "integratedTerminal",
            "args": [
                "--dataset",
                "${workspaceFolder}/data/cracker_box/cracker_box_3",
                "--nerf_path=${workspaceFolder}/external/instant-ngp",
                "--aabb_scale=4",
                "--fps=10",
                "--redo_sfm"
            ],
            "justMyCode": true
        },
        {
            "name": "OnePose feature",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/OnePose/feature_matching_object_detector.py",
            "args": [
                "+experiment=object_detector_custom.yaml",
                "n_ref_view=15"
            ],
            "cwd": "${workspaceFolder}/external/OnePose",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose inference",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/OnePose/inference.py",
            "args": [
                "+experiment=test_GATsSPG_custom.yaml",
                "object_detect_mode=feature_matching",
                "save_wis3d=True"
            ],
            "cwd": "${workspaceFolder}/external/OnePose",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose preprocess",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/OnePose/run.py",
            "args": [
                "+preprocess=sfm_spp_spg_custom.yaml",
                "redo=True"
            ],
            "cwd": "${workspaceFolder}/external/OnePose",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose preprocess V2",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/OnePose/run.py",
            "args": [
                "+preprocess=sfm_spp_spg_sample.yaml",
                "redo=True"
            ],
            "cwd": "${workspaceFolder}/external/OnePose",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Refine",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/detic_utils/refinement.py",
            "args": [
                "--data_path",
                "/home/robot-learning/Projects/ScenePose/data/video_dragon"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Test_Detic",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/external/Detic/demo_v2.py",
            "args": [
                "--config-file",
                "/home/robot-learning/Projects/ScenePose/external/Detic/configs/Detic_LCOCOI21k_CLIP_SwinB_896b32_4x_ft4x_max-size.yaml",
                "--input",
                "/home/robot-learning/Projects/ScenePose/data/video_dragon/images/0001.jpg",
                "--output",
                "/home/robot-learning/Projects/ScenePose/data/video_dragon/seg_vis/0001.jpg",
                "--seg_output",
                "/home/robot-learning/Projects/ScenePose/data/video_dragon/seg/0001.png",
                "--vocabulary",
                "lvis",
                "--opts",
                "MODEL.WEIGHTS",
                "models/Detic_LCOCOI21k_CLIP_SwinB_896b32_4x_ft4x_max-size.pth",
            ],
            "cwd": "${workspaceFolder}/external/Detic",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Prepare ICRA23",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/colmap_utils/data_prepare.py",
            "args": [
                "--src_path",
                "/media/robot-learning/DATA/ws_Haonan/icra23_data/0001",
                "--dst_path",
                "${workspaceFolder}/data/0001"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Prepare MOV",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/colmap_utils/data_prepare.py",
            "args": [
                "--src_path",
                "/home/robot-learning/Projects/ScenePose/data/video_dragon",
                "--dst_path",
                "${workspaceFolder}/data/video_dragon",
                "--src_type",
                "mov",
                "--fps",
                "5"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
}